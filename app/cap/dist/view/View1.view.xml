<mvc:View controllerName="cap.cap.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content />
        	<List
		items="{/Books}"
		headerText="Books">
		<ObjectListItem
			title="{ID}"
			
			numberUnit="{CurrencyCode}">
			
			<ObjectAttribute text="{title}" />
			<ObjectAttribute text="{stock}" />
		</ObjectListItem>
	</List>
    </Page>
</mvc:View>
