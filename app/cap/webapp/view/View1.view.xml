<mvc:View controllerName="cap.cap.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content />
        	<List
		items="{/Books}"
		headerText="Users">
		<ObjectListItem
			title="{id}"
			
			numberUnit="{name}">
			
			<ObjectAttribute text="{username}" />
			<ObjectAttribute text="{email}" />
		</ObjectListItem>
	</List>
    </Page>
</mvc:View>
